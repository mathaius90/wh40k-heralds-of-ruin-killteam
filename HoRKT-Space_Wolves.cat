    <profile id="8daa-c10c-ad88-549e" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" name="Auto-launchers" hidden="false" book="Kill Team List: Space Wolves v3.0a" page="9">
      <characteristics>
        <characteristic characteristicId="21befb24-fc85-4f52-a745-64b2e48f8228" name="Description" value="The model counts as having assault grenades that do not have the One Use rule (i.e. they never run out). These grenades may not be used in the Shooting phase. "/>
      </characteristics>
      <modifiers/>
    </profile>
    <profile id="67c9-7cfa-0564-3d7b" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" name="Auxiliary Grenade Launcher" hidden="false" book="Kill Team List: Space Wolves v3.0a" page="9">
      <characteristics>
        <characteristic characteristicId="21befb24-fc85-4f52-a745-64b2e48f8228" name="Description" value="A tubular device that fires pre-loaded grenades, fixed to the models primary weapon. This attachment allows any grenades to be fired in addition to another weapon in the shooting phase rather than instead of. "/>
      </characteristics>
      <modifiers/>
    </profile>
    <profile id="7ab2-df2e-a59c-a576" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" name="Back Banner" hidden="false" book="Kill Team List: Space Wolves v3.0a" page="1">
      <characteristics>
        <characteristic characteristicId="21befb24-fc85-4f52-a745-64b2e48f8228" name="Description" value="Counts as a &apos;banner&apos; when determining combat results."/>
      </characteristics>
